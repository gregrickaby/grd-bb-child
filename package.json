  },
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "main": "src/index.js",
  "dependencies": {
    "@wordpress/scripts": "^26.6.0"
  },
  "devDependencies": {
    "@wordpress/prettier-config": "^2.18.0",
    "stylelint-config-prettier": "^9.0.5"
  },
  "scripts": {
    "build": "wp-scripts build",
    "dev": "wp-scripts start",
    "format": "wp-scripts format && composer run format",
